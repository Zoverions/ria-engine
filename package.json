{
  "name": "@ria/engine",
  "version": "2.1.0",
  "description": "Resonant Interface Architecture - Core Mathematical Cognitive Enhancement Engine with Novel Enhancements",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "node --test tests/**/*.test.js",
    "test:coverage": "c8 node --test tests/**/*.test.js",
    "build": "node scripts/build.js",
    "build:docs": "node scripts/build-docs.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "benchmark": "node benchmarks/run-benchmarks.js",
    "profile": "node --prof index.js",
    "validate": "node scripts/validate-config.js",
    "demo": "node demos/quick-start.js",
    "demo:quick": "node demos/quick-start.js",
    "demo:full": "node demos/comprehensive-demo.js",
    "demo:enhancements": "node demos/novel-enhancements-demo.js",
    "demo:core": "node demos/core-test.js",
    "demo:all": "npm run demo:quick && npm run demo:core && npm run demo:enhancements && npm run demo:full",
    "docs:serve": "node scripts/serve-docs.js"
  },
  "keywords": [
    "cognitive-enhancement",
    "mathematical-processing",
    "spectral-analysis",
    "fractal-processing",
    "wavelet-analysis",
    "statistical-processing", 
    "antifragile-learning",
    "generative-interventions",
    "multi-sensory-resonance",
    "reinforcement-learning",
    "signal-processing",
    "cognitive-science",
    "attention-research",
    "mathematical-algorithms"
  ],
  "author": "Zoverions Grand Unified Model ZGUM v16.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zoverions/ria-engine.git"
  },
  "bugs": {
    "url": "https://github.com/Zoverions/ria-engine/issues"
  },
  "homepage": "https://github.com/Zoverions/ria-engine#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "import": "./core/index.js"
    },
    "./analytics": {
      "import": "./analytics/index.js"
    },
    "./ml": {
      "import": "./ml/index.js"
    },
    "./biometrics": {
      "import": "./biometrics/index.js"
    },
    "./antifragile": {
      "import": "./antifragile/index.js"
    },
    "./generative": {
      "import": "./generative/index.js"
    },
    "./resonance": {
      "import": "./resonance/index.js"
    }
  },
  "files": [
    "index.js",
    "core/",
    "analytics/",
    "ml/",
    "biometrics/",
    "antifragile/",
    "generative/",
    "resonance/",
    "dist/",
    "docs/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "events": "^3.3.0"
  },
  "devDependencies": {
    "c8": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "ws": "^8.14.2",
    "pako": "^2.1.0"
  },
  "peerDependencies": {
    "canvas": "^2.11.2",
    "puppeteer": "^21.0.0"
  },
  "config": {
    "default_profile": "average",
    "default_environment": "production",
    "analytics_enabled": true,
    "telemetry_enabled": false
  },
  "ria": {
    "engine_version": "2.1.0",
    "api_version": "2.1.0",
    "compatibility": ["2.x"],
    "features": {
      "core": true,
      "analytics": true,
      "biometrics": true,
      "ml": true,
      "mathematical_processing": true,
      "novel_enhancements": true,
      "generative_interventions": true,
      "multi_sensory_resonance": true,
      "antifragile_learning": true
    },
    "mathematical_processors": {
      "spectral": true,
      "fractal": true,
      "wavelet": true,
      "statistical": true
    },
    "cognitive_enhancements": {
      "fracture_index": true,
      "adaptive_resonance": true,
      "reinforcement_learning": true,
      "knowledge_generation": true
    }
  }
}