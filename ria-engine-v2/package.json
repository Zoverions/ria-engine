{
  "name": "@ria/engine",
  "version": "2.0.0",
  "description": "Resonant Interface Architecture - Complete Cognitive Load Reduction Engine",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "node --test tests/**/*.test.js",
    "test:coverage": "c8 node --test tests/**/*.test.js",
    "build": "node scripts/build.js",
    "build:plugins": "node scripts/build-plugins.js",
    "build:docs": "node scripts/build-docs.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "benchmark": "node benchmarks/run-benchmarks.js",
    "profile": "node --prof index.js",
    "validate": "node scripts/validate-config.js",
    "demo": "node demos/quick-start.js",
    "demo:quick": "node demos/quick-start.js",
    "demo:full": "node demos/comprehensive-demo.js",
    "demo:platforms": "node demos/platform-demo.js",
    "demo:financial": "node demos/financial-metrics-demo.js",
    "demo:all": "npm run demo:quick && npm run demo:platforms && npm run demo:full",
  "demo:erc20": "node demos/erc20-bot-demo.js",
  "demo:eth-backtest": "node demos/eth-backtest.js",
  "data:eth-csv": "node scripts/fetch-eth-csv.js",
  "data:normalize": "node ria-engine-v2/scripts/normalize-eth-csv.js ria-engine-v2/data/eth_hourly_raw.csv ria-engine-v2/data/eth_hourly.csv",
  "backtest:eth:csv": "CSV=ria-engine-v2/data/eth_hourly.csv node ria-engine-v2/demos/eth-backtest.js",
    "backtest:eth": "CSV=ria-engine-v2/data/eth_hourly.csv node ria-engine-v2/demos/eth-backtest.js",
    "docs:serve": "node scripts/serve-docs.js"
  },
  "keywords": [
    "cognitive-load",
    "attention",
    "ui-adaptation",
    "real-time",
    "analytics",
    "biometrics",
    "machine-learning",
    "accessibility",
    "productivity",
    "user-experience"
  ],
  "author": "Zoverions Grand Unified Model ZGUM v16.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zoverions/ria-engine.git"
  },
  "bugs": {
    "url": "https://github.com/Zoverions/ria-engine/issues"
  },
  "homepage": "https://github.com/Zoverions/ria-engine#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "import": "./core/index.js"
    },
    "./plugins": {
      "import": "./plugins/index.js"
    },
    "./analytics": {
      "import": "./analytics/index.js"
    },
    "./ml": {
      "import": "./ml/index.js"
    },
    "./biometrics": {
      "import": "./biometrics/index.js"
    }
  },
  "files": [
    "index.js",
    "core/",
    "plugins/",
    "analytics/",
    "ml/",
    "biometrics/",
    "dist/",
    "docs/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "events": "^3.3.0"
  },
  "devDependencies": {
    "c8": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "ws": "^8.14.2",
    "pako": "^2.1.0",
    "ethers": "^6.13.2"
  },
  "peerDependencies": {
    "canvas": "^2.11.2",
    "puppeteer": "^21.0.0"
  },
  "config": {
    "default_profile": "average",
    "default_environment": "production",
    "analytics_enabled": true,
    "telemetry_enabled": false
  },
  "ria": {
    "engine_version": "2.0.0",
    "api_version": "2.0.0",
    "compatibility": ["1.x"],
    "features": {
      "core": true,
      "analytics": true,
      "biometrics": true,
      "ml": true,
      "plugins": true,
      "enterprise": true
    },
    "platforms": {
      "web": true,
      "electron": true,
      "mobile": true,
      "figma": true,
      "vscode": true
    },
    "deployment": {
      "cloud": true,
      "on_premise": true,
      "edge": true
    }
  }
}